{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "paths": {
      "@config": ["src/config/*"],
      "@routes": ["src/routes/*"]
    }
  },
  "include": ["src/**/*"],
}